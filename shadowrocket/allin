[URL Rewrite]
^https:\/\/app-measurement\.com\/config\/app\/1?(.*?)*$ - reject
http-response ^https?://api\.revenuecat\.com/.+/(receipts$|subscribers/?(.*?)*$) header-del X-RevenueCat-ETag
http-response ^https?://api\.revenuecat\.com/.+/(receipts$|subscribers/?(.*?)*$) header-del x-revenuecat-etag
^https?:\/\/[\w-]+\.googlevideo\.com\/initplayback.+&oad REJECT-200 
^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) url script-response-body https://raw.githubusercontent.com/2023djash/2023djash/main/js/RevenuecatHeji.js
^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) url script-request-header https://raw.githubusercontent.com/2023djash/2023djash/main/js/RevenuecatHeji.js
^(http|https):\/\/api\.revenuecat\.com\/v1\/subscribers\/(?!.*\/(offerings|attributes|adservices_attribution)$).* https://api.revenuecat.com/v1/product_entitlement_mapping 307
########################################

[Header Rewrite]
http-response ^https?://api\.revenuecat\.com/.+/(receipts$|subscribers/?(.*?)*$) header-del X-RevenueCat-ETag
http-response ^https?://api\.revenuecat\.com/.+/(receipts$|subscribers/?(.*?)*$) header-del x-revenuecat-etag
########################################

